               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="product_credentials"
               enableSelectByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="products"
               enableSelectByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="roles"
               enableSelectByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="shops"
               enableSelectByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="system_configs"
               enableSelectByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableCountByExample="false"/>

        <table catalog="mmo_schema" tableName="users"
               enableSelectByExample="false" enableUpdateByExample="false"
